# Basics section
APP_NAME=site

# enter your app name
ENVIRONMENT=local

# enter your environment. as example: local/dev/stage
# enter ${APP_NAME} without spec symbols. example: "test-app" => "testapp"
COMPOSE_PROJECT_NAME=sitelocal

# Enter your domain
DOMAIN=site.loc

# App path
APP_FOLDER=src

# Ports section
# enter custom nginx port 80
NGINX_PORT=80

# enter custom php port 9000
PHP_PORT=9000

# enter Composer initial settings
COMPOSER_NAME=evgenylapshin/docker
COMPOSER_DESCRIPTION=Docker-Template
COMPOSER_TYPE=project
COMPOSER_LICENSE=MIT
COMPOSER_AUTOLOAD=PSR-4

# TimeZone
TIMEZONE=Europe/Moscow

# Postgres database
POSTGRES_DB=public
POSTGRES_USER=root
POSTGRES_PASSWORD=root
POSTGRES_PORT=5432
